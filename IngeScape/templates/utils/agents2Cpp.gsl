{
.   if parameter.type = "DOUBLE"
    if (qFuzzyCompare(_$(prop_name), value) == 0)
.   elsif (parameter.type = "STRING" | parameter.type = "DATA")
    if ((_$(prop_name) != value) || (_$(prop_name).isNull() != value.isNull()))
.   else
